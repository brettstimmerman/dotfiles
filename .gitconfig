[alias]
    alias = !git config --list | grep 'alias\\.' | sed 's/alias\\.\\([^=]*\\)=\\(.*\\)/\\1\t=> \\2/' | sort
    amend = commit --amend
    br = branch
    bra = branch -a
    ci = commit
    co = checkout
    ll = log --graph --abbrev-commit --max-count=15
    r = checkout --
    remotes = remote -v
    st = status

[color]
    ui = auto

[core]
    editor = subl -w
    excludesfile = ~/.gitignore

[format]
    pretty = %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %C(blue)<%an>%Creset

[push]
    default = tracking

[user]
